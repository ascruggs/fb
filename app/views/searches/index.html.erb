<%= form_tag(lookup_searches_path, :method => "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>

<% if params[:q].any? %>
	<p>You searched for "<%= params[:q] %>"</p>
<% end %>

<% if @results && @results["data"].any %>
	<ul>
	<% @results["data"].each do |result| %>
		<li><%= result.inspect %></li>
	<% end %>
	</ul>
	<%= @results["paging"].inspect %>
<% end %>